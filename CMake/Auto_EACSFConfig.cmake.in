# - Config file for the Auto_EACSF package
# It defines the following variables
#  Auto_EACSF_INCLUDE_DIRS - include directories
#  Auto_EACSF_LIBRARIES    - libraries generated by Auto_EACSF
#  Auto_EACSF_EXECUTABLES   - the Auto_EACSF executable
 
# Compute paths

get_filename_component(Auto_EACSF_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

file(TO_CMAKE_PATH "${Auto_EACSF_CMAKE_DIR}/../../../include" Auto_EACSF_INCLUDE_DIRS)
file(TO_CMAKE_PATH "${Auto_EACSF_CMAKE_DIR}/../../" Auto_EACSF_LIBRARY_DIRS)

link_directories(${Auto_EACSF_LIBRARY_DIRS})

set(Auto_EACSF_LIBRARIES Auto_EACSFLib)

find_program(Auto_EACSF_EXECUTABLE
  NAMES Auto_EACSF
  PATHS
    "${Auto_EACSF_CMAKE_DIR}/../../../bin"    
  NO_DEFAULT_PATH
  )

message(STATUS "\t Auto_EACSF_EXECUTABLE=" ${Auto_EACSF_EXECUTABLE})
message(STATUS "\t Auto_EACSF_INCLUDE_DIRS=" ${Auto_EACSF_INCLUDE_DIRS})
message(STATUS "\t Auto_EACSF_LIBRARIES=" ${Auto_EACSF_LIBRARIES})
message(STATUS "\t Auto_EACSF_LIBRARY_DIRS=" ${Auto_EACSF_LIBRARY_DIRS})